
extends layout

block content
	div.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
		div.container
			div.navbar-header
				button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='#') Project name
			div.collapse.navbar-collapse
				ul.nav.navbar-nav
					li.active
						a(href='#') Home
					li
						a(href='#') MyHistory
					li#myprofile
						a() MyProfile
					li#search
						a() Search
			button#btn-logout.btn.btn-primary.glyphicon.glyphicon-log-out
			button#btn-controlpanel.btn.btn-primary.glyphicon.glyphicon-tasks
				//i.icon-lock.icon-white
				//| Sign Out
	div#content
		div#treediv
			div#searchdiv.btn-group
				//span.input-group-addon @
				input#treesearch.input-xlarge.form-control(type='text')
				span#searchclear.glyphicon.glyphicon-search
			div#giscontent
		div#canvascontainer
			canvas#mapcanvas
	include modals/profile-account
	include modals/alert
	include modals/confirm
	include modals/progressbar
	include modals/treeview-download
	include modals/search
	include modals/contextmenu
	include modals/layercontroller
	//div#footer
		div.container
			p.text-muted @firegnu 2014

